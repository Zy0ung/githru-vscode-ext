{
  "name": "githru-vscode-ext",
  "displayName": "githru-vscode-ext",
  "description": "vscode extension module for githru-vscode-ext",
  "publisher": "githru",
  "repository": {
    "url": "https://github.com/githru/githru-vscode-ext",
    "type": "git"
  },
  "version": "0.7.2",
  "engines": {
    "vscode": "^1.67.0"
  },
  "icon": "images/githru_logo_v0.2.png",
  "author": {
    "name": "team githru"
  },
  "categories": [
    "Other",
    "SCM Providers"
  ],
  "keywords": [
    "githru",
    "git",
    "GitHub",
    "log",
    "visualization",
    "visual analytics"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "menus": {
      "scm/title": [
        {
          "command": "githru.command.launch",
          "when": "scmProvider == git",
          "group": "navigation@2"
        }
      ]
    },
    "commands": [
      {
        "command": "githru.command.launch",
        "title": "Open Githru View",
        "icon": "images/logo.png",
        "category": "Githru"
      },
      {
        "command": "githru.command.login.github",
        "title": "Login with Github",
        "category": "Githru"
      },
      {
        "command": "githru.command.reset.github_auth",
        "title": "Reset GitHub Authentication saved previously",
        "category": "Githru"
      }
    ],
    "configuration": {
      "title": "Githru",
      "properties": {
        "githru.theme": {
          "type": "string",
          "default": "githru",
          "description": "Insert your theme name: githru, hacker-blue, aqua, cotton-candy, mono"
        }
      }
    }
  },
  "scripts": {
    "prebuild": "npm run --prefix ../view/ build",
    "build": "npm run compile",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map && vsce package --no-dependencies",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "lint:fix": "eslint src --ext ts --fix",
    "test": "jest"
  },
  "dependencies": {
    "@githru-vscode-ext/analysis-engine": "^0.7.2",
    "@octokit/rest": "^20.1.0",
    "node-fetch": "^3.4.1"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/jest": "^29.7.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "^20.12.12",
    "@types/vscode": "^1.67.0",
    "@types/webpack": "^5.28.5",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "@vscode/test-electron": "^2.4.0",
    "copy-webpack-plugin": "^12.0.0",
    "data-uri-to-buffer": "^4.0.1",
    "eslint": "^9.4.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-unused-imports": "^4.0.0",
    "formdata-polyfill": "^4.0.10",
    "glob": "^10.3.10",
    "jest": "^29.7.0",
    "mocha": "^9.2.2",
    "prettier": "^3.0.1",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.1",
    "typescript": "^5.4.5",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4"
  }
}
